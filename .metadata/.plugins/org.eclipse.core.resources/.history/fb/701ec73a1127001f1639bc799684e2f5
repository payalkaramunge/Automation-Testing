
//		Variables and Data types

package Day_2_java;

public class FirstJavaProgram
{
	public static void main (String args[])
	{
		int a = 100;
		System.out.println(a);
		
		a=200;
		System.out.println(a);
		
		int b,c;
		b=10;
		c=20;
		System.out.println("value of b:" +b);
		System.out.println("value of b:" + b);
	}

}
